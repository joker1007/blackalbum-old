#setting
  .player
    label Player:
    select#player_select
      != player_options
    br
    a.player_new(href="/player")
      追加
    |
    a.player_edit(href="#")
      編集
    |
    a.player_destroy(href="#")
      削除

  .order
    label Order:
    select#order
      != order_options

  .search
    form.search_form(action="/entries/search", method="POST")
      input(type="text", name="q", size="60", value=req.q ? req.q : "")
      input(type="submit", value="検索")

.entries
  != partial('list')

script
  $("#order_#{session.order[2]}").attr("selected", "selected");
